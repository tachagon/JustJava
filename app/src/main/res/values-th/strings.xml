<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the application. [CHAR LIMIT=12] -->
    <string name="app_name">ก็แค่ Java</string>

    <!-- Hint text display in the empty field for the user's name [CHAR LIMIT=20] -->
    <string name="name">ชื่อ</string>

    <!-- Header text for the list of toppings [CHAR LIMIT=20] -->
    <string name="toppings">ท็อปปิ้ง</string>

    <!-- Hint text display in the CheckBox [CHAR LIMIT=20] -->
    <string name="whipped_cream">วิปครีม</string>

    <!-- Hint text display in the CheckBox [CHAR LIMIT=20] -->
    <string name="chocolate">ช็อคโกแลต</string>

    <!-- Header text for the list of quantity [CHAR LIMIT=20] -->
    <string name="quantity">จำนวน (ถ้วย)</string>

    <!-- Hint text display in initial quantity value [CHAR LIMIT=5] -->
    <string name="initial_quantity_value">2</string>

    <!-- Hint text display in the order button [CHAR LIMIT=20] -->
    <string name="order">สั่งซื้อ</string>

    <!--
     Name for the order summary. It will be shown in the format of "Name: Amy"
     where Amy is the user's name. [CHAR LIMIT=NONE]
     -->
    <string name="order_summary_name">ชื่อ: <xliff:g example="Amy" id="name">%s</xliff:g></string>

    <!--
      Whipped cream topping for the order summary. It will be shown in the format of
      "Add whipped cream? true" or "Add whipped cream? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_whipped_cream">เพิ่มวิปครีมด้วยหรือไม่? <xliff:g example="true" id="addWhippedCream">%b</xliff:g></string>

    <!--
      Chocolate topping for the order summary. It will be shown in the format of
      "Add chocolate? true" or "Add chocolate? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_chocolate">เพิ่มช็อคโกแลตด้วยหรือไม่? <xliff:g example="true" id="addChocolate">%b</xliff:g></string>

    <!--
      Quantity of coffee cups for the order summary. It will be shown in the format of
      "Quantity: 2", where 2 is the number of cups ordered. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_quantity">จำนวนรวม: <xliff:g example="2" id="quantity">%d</xliff:g> ถ้วย</string>

    <!--
      Total price for the order summary. It will be shown in the format of
      "Total: $10" where $10 is the price. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_price">ราคารวม: <xliff:g example="$10" id="price">%s</xliff:g></string>

    <!-- Thank you message for the order summary [CHAR LIMIT=NONE] -->
    <string name="thank_you">ขอบคุณครับ!</string>

    <!--
      Subject line for the order summary email. It will be in the format of
      "Just Java order for Amy" where Amy is the user's name. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_email_subject">ก็แค่ Java รายการสั่งซื้อสำหรับ <xliff:g example="Amy" id="name">%s</xliff:g></string>

</resources>
